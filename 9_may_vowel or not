public class Vowel{

	public static void main(String[] args) {
		
		char ch = 'i';
		
		if(ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u'){
		  System.out.println("This is vowel number");
                }
		else{
		  System.out.println("This is not a viwel number");
	        }

	}

}


output:
i This is vowel number 
