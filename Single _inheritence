package inheritence;

public class Add
{
    public static void demo1()
    {
    int a=10;
    int b=2;
    int c=a+b;
    System.out.println(c);
    }
   
}
 
------------------------------------------------------------

package inheritence;

public class sub extends Add
{
public static void demo()
{
int a=10;
int b=2;
int c=a-b;
System.out.println(c);

}
public static void main(String arg[])
{
demo1();
demo();
}

}
