import java.util.Scanner;

public class prime {
    public static void main(String arg[]){
        int num,count=0;
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the number");
        num=sc.nextInt();
        for(int i=1;i<=num;i++){
            if(num%i==0){
                count++;
            }
        }
        if(count==2){
            System.out.println("prime number");
        }
        else{
            System.out.println("not prime number");
        }
    }
    
}

